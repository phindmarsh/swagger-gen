{
  "name": "wave-framework/swagger-gen",
  "description": "Generates a swagger spec file from a wave app",

  "require": {
      "justinrainbow/json-schema": "dev-fix-93@dev"
  },

  "repositories": [
      {
          "type": "vcs",
          "url": "https://github.com/co3k/json-schema"
      }
  ],

  "bin": ["bin/swagger-gen"],

  "autoload": {
    "psr-4": { "Wave\\Swagger\\Generator\\": "src/" }
  }

}
